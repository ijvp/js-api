{
  "name": "iris-backend",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:dev": "jest --watch --silent --runInBand",
    "dev": "export NODE_ENV=development && tsx watch src/server.ts",
    "tunnel": "ngrok http --url=oriented-better-pigeon.ngrok-free.app 8080",
    "build": "tsc",
    "start": "export NODE_ENV=production && node dist/server.js",
    "docker": "docker network create dashboard-network && docker-compose -f docker/docker-compose.yml up --build -d && docker-compose up",
    "dockerDown": "docker-compose down && docker-compose -f docker/docker-compose.yml down && docker network rm dashboard-network",
    "redis": "docker-compose -f docker/docker-compose.yml up -d",
    "redisDown": "docker-compose -f docker/docker-compose.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shopify/shopify-api": "^11.6.1",
    "@shopify/shopify-app-express": "^5.0.9",
    "@shopify/shopify-app-session-storage-redis": "^4.2.2",
    "axios": "^1.2.6",
    "clean": "^4.0.2",
    "connect-mongo": "^5.0.0",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.29.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "google-ads-api": "^17.1.0-rest-beta",
    "googleapis": "^110.0.0",
    "http": "^0.0.1-security",
    "ioredis": "^5.4.2",
    "moment": "^2.29.4",
    "mongoose": "^6.9.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "pg": "^8.13.1",
    "pino": "^8.12.0",
    "pino-pretty": "^10.0.0",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/passport": "^1.0.17",
    "docker-compose": "^1.1.0",
    "jest": "^29.2.5",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
